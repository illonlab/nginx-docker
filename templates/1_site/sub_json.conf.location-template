location /${NGINX_SITE_1_SUB_JSON_URL} {
  if ($hack = 1) {return 404;}
  proxy_redirect off;
  proxy_set_header Host $host;
  proxy_set_header X-Real-IP $remote_addr;
  proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
  proxy_pass http://${NGINX_SITE_1_SUB_JSON_HOST}:${NGINX_SITE_1_SUB_JSON_PORT}/${NGINX_SITE_1_SUB_JSON_URL};
  break;
}